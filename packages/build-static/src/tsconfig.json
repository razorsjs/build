{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "exclude": [
      "node_modules"
    ],
    "strict": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
  }
}
